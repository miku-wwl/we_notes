## for ... in
p("for1 ... start");

for i in range(1,10) {
  p(i);
}

p("for1 ... end");


p("for2 ... start");

for i in range(1,10,2) {
  if i == 5 {
     continue;
  }
  if i == 7 {
     break;
  }
  p(i);
}

p("for2 ... end");

## 九九乘法表
p("for3 ... start");

for i in range(1,10) {
   for j in range(1,i+1) {
      print("#{i} * #{j} = #{i*j} ")
   }
   p();
}

p("for3 ... end");